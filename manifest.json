{
    "name": "LinkedInLinks",
    "description": "Easily copy/paste linkedin field information",
    "version": "1.0",
    "manifest_version": 3,
    "host_permissions": [
        "*://*.google.com"
    ],
    "permissions": ["activeTab", "scripting", "cookies"],
    "action":{
        "default_popup": "popup.html"
    }
}